<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wjl.student_management.dao.LoginDao">



    <select id="selectAll" resultType="com.wjl.student_management.entity.User">
       select * from user
       where username = #{username}
       and password = #{password}

    </select>

</mapper>